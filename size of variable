#include<stdio.h>
int main()
{
	int intType;
	float floatType;
	double doubleType;
	char charType;
	printf("size of int:%zubytes\n",sizeof(intType));
	printf("size of float: %zubytes\n",sizeof(floatType));
	printf("size of double: %zubytes\n",sizeof(doubleType));
	printf("size of char: %zubytes\n",sizeof(charType));
	return 0;
}
